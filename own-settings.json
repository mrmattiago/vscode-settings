{
  "explorer.openEditors.visible": 0,
  "editor.snippetSuggestions": "top",
  "emmet.showAbbreviationSuggestions": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.fontLigatures": true,
  "terminal.integrated.fontSize": 14,
  "files.autoSave": "off",
  "editor.fontFamily": "'MesloLGS NF', 'Cascadia Code', 'Fira Code'",
  "markdown.preview.fontSize": 16,
  "editor.detectIndentation": true,
  "eslint.enable": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.suggestSelection": "first",
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[css]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {},
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[gherkin]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "comment.block"],
        "settings": {
          "fontStyle": "italic"
          // "foreground": "#ff1493"
        }
      },
      {
        "scope": [
          "keyword.operator.logical",
          "keyword.operator.arithmetic",
          "keyword.operator.assignment",
          "keyword.operator.bitwise"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "cSpell.userWords": [
    "deno",
    "feathersjs",
    "middlewares",
    "socketio",
    "upsert",
    "upvote"
  ],
  "editor.lineHeight": 30,
  "editor.fontSize": 18,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.colorTheme": "Super One Dark",
  "workbench.iconTheme": "helium-icon-theme",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorWidth": 3,
  "editor.cursorBlinking": "expand",
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!graphql",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!less",
    "!php",
    "!pug",
    "!python",
    "!rust",
    "!scala",
    "!scss",
    "dart",
    "styled-css"
  ],
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "editor.linkedEditing": true,
  "debug.console.fontSize": 10,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "workbench.fontAliasing": "antialiased",
  "editor.codeLensFontFamily": "Anonymous Pro",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.sendKeybindingsToShell": true,
  "http.proxyAuthorization": null,
  "http.proxyStrictSSL": true,
  "http.proxySupport": "on",
  "workbench.productIconTheme": "developer-icons",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "window.zoomLevel": -0.5,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.editor.untitled.hint": "hidden",
  "editor.find.cursorMoveOnType": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "workbench.sideBar.location": "right",
  "npm.enableRunFromFolder": true,
  "debug.javascript.codelens.npmScripts": "never",
  "git.confirmSync": false,
  "diffEditor.maxComputationTime": 0,
  "editor.language.brackets": [
  
  ]
}
